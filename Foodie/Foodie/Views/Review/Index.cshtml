@model List<Foodie.Models.Review>
@{
    ViewBag.Title = "Reviews";
}

<h2>Index</h2>
    @for (int i = 0; i < Model.Count; i++)
    {
  
            <ul>
                    <li>
                        @Html.LabelFor(m => m[i].UserName)
                        @Html.DisplayTextFor(m => m[i].UserName)
                    </li>
                    <li>
                        @Html.LabelFor(m => m[i].DatePosted)
                        @Html.DisplayFor(m => m[i].DatePosted)
                    </li>
                    <li>
                        @Html.LabelFor(m => m[i].RestaurantName)
                        @Html.DisplayFor(m => m[i].RestaurantName)
                    </li>
                    <li>
                        @Html.LabelFor(m => m[i].ReviewText)
                        @Html.DisplayFor(m => m[i].ReviewText)
                    </li>   
           </ul>      
  
    }